{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:import/typescript"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "import/extensions": "off",
    "max-len": ["warn", {
      "code": 120,
      "tabWidth": 2,
      "comments": 200,
      "ignoreUrls": true,
      "ignoreStrings": false
  }]
  }
}
